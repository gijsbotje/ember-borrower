<h2>Loans Index</h2>
<table class="table">
  <thead>
    <tr>
      <th>Description</th>
      <th>Borrowed since</th>
      <th>Notes</th>
      <th>Returned?</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    {{#each model as |loan|}}
      {{loans/loan-row
        loan=loan
        save=(action save)
      }}
    {{/each}}
  </tbody>
</table>